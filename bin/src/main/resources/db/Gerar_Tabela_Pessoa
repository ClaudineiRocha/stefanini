--Script para criar tabela de armazemento de pessoa 
 CREATE TABLE TB_PESSOA (
  		CD_PESSOA int NOT NULL,
  		NM_PESSOA VARCHAR(100) NOT NULL, 
		EMAIL VARCHAR(100),
		DT_NASCIMENTO VARCHAR(8),
		NATURALIDADE VARCHAR(50),
		NASCIONALIDADE VARCHAR(50),
		NR_DOCUMENTO VARCHAR(20) NOT NULL,
		SEXO VARCHAR(50),
		CEP VARCHAR(9),
		DT_CADASTRO VARCHAR(9),
		DT_ATUALIZACAO VARCHAR(9),
      CONSTRAINT PK_PESSOA PRIMARY KEY (CD_PESSOA,NR_DOCUMENTO));